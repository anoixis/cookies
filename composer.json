{
    "name": "tank/cookies",
    "description": "Cookies for PSR-7 HTTP Message Interface.",
    "license": "MIT",
    "keywords": ["psr7", "psr-7", "cookies"],
    "authors": [
        {
            "name": "Matthew Kilgore",
            "email": "matthew@kilgore.dev"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-pcre": "*",
        "psr/http-message": "^1"
    },
    "autoload": {
        "psr-4": {
            "Tank\\Cookies\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.8",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tank\\Cookies\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
